<keymap version="1" name="Windows copy" parent="$default">
  <action id="ActivateBookmarksToolWindow" />
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
    <keyboard-shortcut first-keystroke="alt 2" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
  <action id="ExpandAllToLevel1">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="1" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad1" />
    <keyboard-shortcut first-keystroke="ctrl alt 1" />
  </action>
  <action id="ExpandAllToLevel2">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="2" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad2" />
    <keyboard-shortcut first-keystroke="ctrl alt 2" />
  </action>
  <action id="ExpandAllToLevel3">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="3" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad3" />
    <keyboard-shortcut first-keystroke="ctrl alt 3" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
  </action>
  <action id="Kotlin.RunScratch" />
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>